<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title><%= title %></title>
    <!-- Bootstrap CSS -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css"
      rel="stylesheet"
    />

    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <% for(let i = 0; i < getUserGame.length ; i++) { %>
    <p><%= getUserGame[i].username %></p>

    <% }%>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">id</th>
          <th scope="col">username</th>
          <th scope="col">email</th>
          <th scope="col">gen_random</th>
        </tr>
      </thead>
      <tbody>
        <% for(let i = 0; i < getUserGame.length ; i++) { %>
        <tr>
          <th scope="row"><%= getUserGame[i].id %></th>
          <td><%= getUserGame[i].username %></td>
          <td><%= getUserGame[i].email %></td>
          <td>@<%= getUserGame[i].generate_random %></td>
        </tr>
        <% }%>
      </tbody>
    </table>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js"></script>
  </body>
</html>
